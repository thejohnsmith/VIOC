{
  "name": "vioc",
  "version": "1.0.0",
  "description": "Valvoline Instant Oil Change static development",
  "main": "app.js",
  "devDependencies": {
    "browser-sync": "^2.11.1",
    "browserify": "latest",
    "watchify": "latest",
    "onchange": "latest"
  },
  "scripts": {
    "watch-js": "watchify static/js/dataService.js -o static/js/bundle.js -dv",
    "build-js": "browserify static/js/dataService.js | uglifyjs --compress --output static/js/bundle.js -d",
    "post-js": "onchange static/js/bundle.js-- npm",
    "start": "node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thejohnsmith/vioc.git"
  },
  "keywords": [
    "vioc"
  ],
  "author": "Epsilon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thejohnsmith/vioc/issues"
  },
  "homepage": "https://github.com/thejohnsmith/vioc#readme"
}
